{
  "name": "template-api",
  "version": "1.0.1",
  "main": "server.js",
  "license": "MIT",
  "config": {
    "jest": "--coverage --passWithNoTests --ci --verbose --forceExit --detectOpenHandles --colors",
    "integration_config": "-i -c integration.config.json",
    "unit_coverall": "tests/unit/coverage/lcov.info",
    "integration_coverall": "tests/integration/coverage/lcov.info",
    "path_dist": "dist",
    "path_source": "src",
    "path_server": "src/server",
    "back_start": "(command -v back >/dev/null 2>&1) && (back start) || ((command -v node_modules/.bin/back >/dev/null 2>&1) && (node_modules/.bin/back start))",
    "back_dev": "((command -v back >/dev/null 2>&1) && (back -d)) || ((command -v node_modules/.bin/back >/dev/null 2>&1) && (node_modules/.bin/back -d))",
    "back_build": "((command -v back >/dev/null 2>&1) && (back -b)) || ((command -v node_modules/.bin/back >/dev/null 2>&1) && (node_modules/.bin/back -b))"
  },
  "scripts": {
    "tsc": "tsc",
    "start": "/bin/sh -c \"$npm_package_config_back_start\"",
    "container-start": "/bin/sh -c \"$npm_package_config_back_start\"",
    "dev": "/bin/sh -c \"$npm_package_config_back_dev\"",
    "build": "/bin/sh -c \"$npm_package_config_back_build\"",
    "export": "next export",
    "test": "jest $npm_package_config_jest",
    "integration": "jest $npm_package_config_integration_config $npm_package_config_jest",
    "test:coveralls": "jest $npm_package_config_jest && coveralls < $npm_package_config_unit_coverall && rm -rf ./tests/unit/coverage/coverage",
    "integration:coveralls": "jest $npm_package_config_integration_config $npm_package_config_jest && coveralls < $npm_package_config_integration_coverall && rm -rf ./tests/integration/coverage/coverage"
  },
  "dependencies": {
    "@backapirest/any": "latest",
    "@flexiblepersistence/service": "latest",
    "@flexiblepersistence/database-info": "latest",
    "@midware/mauth": "latest",
    "clsx": "latest",
    "coveralls": "latest",
    "dotenv": "latest",
    "express-rate-limit": "latest",
    "flexiblepersistence": "latest",
    "helmet": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "reflect-metadata": "latest",
    "thread-promises": "latest",
    "webpack": "latest"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@types/cors": "latest",
    "@types/jest": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-jest": "latest",
    "babel-plugin-root-import": "latest",
    "babel-plugin-styled-components": "latest",
    "enzyme": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-filenames": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-prettier": "latest",
    "jest": "latest",
    "nodemon": "latest",
    "prettier": "latest",
    "sucrase": "latest",
    "ts-jest": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  }
}
